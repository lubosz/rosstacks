  navigation_experimental: {version: 0.1.0}
  physics_ode: {version: 1.0.4}
  point_cloud_perception: {version: 0.1.0}
  pr2_apps:
    _rules: *id001
    version: 0.2.1
  pr2_arm_navigation:
    _rules: *id001
    version: 0.2.1
  pr2_arm_navigation_apps:
    _rules: *id001
    version: 0.2.0
  pr2_arm_navigation_tests:
